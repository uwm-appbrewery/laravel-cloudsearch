{
  "name": "torann/laravel-cloudsearch",
  "type": "library",
  "description": "Index and search Laravel models on Amazon's CloudSearch.",
  "keywords": [
    "cloudsearch",
    "aws",
    "amazon cloudsearch",
    "eloquent",
    "laravel",
    "search"
  ],
  "homepage": "https://github.com/Torann/laravel-cloudsearch",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "torann@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0",
    "illuminate/config": "^6.0",
    "aws/aws-sdk-php": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelCloudSearch\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelCloudSearch\\LaravelCloudSearchServiceProvider"
      ]
    }
  }
}
